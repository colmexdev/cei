<table id="index-panel" style="margin-top:35px;word-break:normal;">
	<thead>
	</thead>
	<tbody>
		<% @fields.each do |f| %>
		<tr class="row-ind">
			<td class="ind-cell marker"><%= f[1] %></td>
			<td class="ind-cell left-align"><%= raw(@obj[f[0]]) %></td>
		</tr>
		<% end %>
		<% @imgs.each do |f| %>
		<tr class="row-ind">
			<td class="ind-cell"><%= f[1] %></td>
			<td class="ind-cell left-align"><%= @obj.attributes[f[1]].url.include?(".pdf") ? (raw "<object type=\"application/pdf\" data =\"" + @obj.attributes[f[1]].url + "\"><embed src=\"" + @obj.attributes[f[1]].url") + "\" type=\"application/pdf\"></object>") : (image_tag(@obj.attributes[f[1]].url)) %></td>
			<% end %>
		</tr>
		<% end %>
	</tbody>
</table>
