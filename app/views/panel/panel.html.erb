<% if admin_signed_in? %>
<div class="cont-gest">

	<div class="barra-gest">
		<div id="brand-gest">
			<%= image_tag("colmex.png", class: "img-brand") %>
			<div class="toggle-panel" id="but-toggle">
				<div class="bars-toggle" id="bar-s"></div>
				<div class="bars-toggle" id="bar-m"></div>
				<div class="bars-toggle" id="bar-i"></div>
			</div>
			<div class="toggle-panel" id="mask-toggle" onmouseenter="changeToggle(0);" onmouseleave="changeToggle(1);" onclick="slideMenu();">
			</div>
		</div>
		<div id="title-gest">
			<div id="opciones-gest">
				<span class="glyphicon glyphicon-plus" id="glyph-new"></span>
				<span class="glyphicon glyphicon-arrow-left" id="glyph-back"></span>
				<span class="glyphicon glyphicon-home" id="glyph-home"></span>
				<span class="glyphicon glyphicon-pencil" id="glyph-edit"></span>
			</div>
			<span id="text-title-gest">Panel gestor de contenido</span>
		</div>
	</div>


	<div class="body-gest">
		<div id="menu-lat">
			<ul id="ops-lat">
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Sliders", :offset => 0) %>')">Sliders</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Colaboradores", :offset => 0) %>')">Colaboradores</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Patrocinadores", :offset => 0) %>')">Patrocinadores</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Proyectos de investigación", :offset => 0) %>')">Proyectos de investigación</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Sesiones de seminario", :offset => 0) %>')">Sesiones de seminario</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Taller", :offset => 0) %>')">Taller</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Documentos de investigación", :offset => 0) %>')">Documentos de investigación</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Noticias de investigación", :offset => 0) %>')">Noticias de investigación</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Entrada de blog", :offset => 0) %>')">Entrada de blog</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Libros", :offset => 0) %>')">Libros</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Consultas bibliográficas", :offset => 0) %>')">Consultas bibliográficas</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Noticias", :offset => 0) %>')">Noticias</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Videos", :offset => 0) %>')">Videos</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Aliados", :offset => 0) %>')">Aliados</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Usuarios gestores", :offset => 0) %>')">Usuarios gestores</a>
				</li>
				<li class="op-gest">
				<a class="link-panel" onclick="hideLink(event,'#ops-lat','<%= panel_index_path(:set => "Catálogo de sitios", :offset => 0) %>')">Catálogo de sitios</a>
				</li>

				<li class="op-gest">
				<a class="link-panel" href="<%= destroy_admin_session_path %>" data-method="DELETE" rel="nofollow">Cerrar sesión</a>
				</li>
			</ul>
		</div>
		<div id="graphs-gest">
		<p id="notice"><%= notice %></p>
		
		</div>

	</div>
</div>

<div id="tooltip-backend">
</div>

<script type="text/javascript">
	$.ajax({
		url: "<%= panel_princ_path(notice: params[:notice] || "") %>",
		success: function(result){
			return true;
		}
	});
</script>

<% end %>
